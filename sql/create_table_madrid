  website STRING,
  ticket_vendor STRING,
  annual_visitors NUMERIC,
  capacity_final NUMERIC,
  atp NUMERIC,
  gtv NUMERIC,
  size_segment STRING,
  ownership STRING,
  rfp STRING,
  enrichment_status STRING,
  notes STRING,
  last_updated TIMESTAMP
);
